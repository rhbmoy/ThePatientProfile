<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="git.combmoy01firebase.httpsgithub.patientprof.LabResults">


    <TextView
        android:id="@+id/bloodDate"
        android:padding="15dp"
        android:background="@drawable/input_outline"
        android:text="Date Created"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/bloodBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lipid Profile Results"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@id/bloodDate"/>

    <TextView
        android:id="@+id/a1cDate"
        android:padding="15dp"
        android:background="@drawable/input_outline"
        android:text="Date Created"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bloodBtn"/>

    <Button
        android:id="@+id/a1cBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HBA-1C Results"
        android:layout_below="@+id/a1cDate"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <WebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:visibility="invisible">

        </WebView>

    </RelativeLayout>


</RelativeLayout>
